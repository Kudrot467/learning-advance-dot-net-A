
@{
    ViewBag.Title = "detailsUser";
}

<h2>detailsUser</h2>
<div>
    <table >
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
        </tr>
        @foreach (var item in Model.users)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.email</td>
            </tr>
        }
    </table>
</div>

